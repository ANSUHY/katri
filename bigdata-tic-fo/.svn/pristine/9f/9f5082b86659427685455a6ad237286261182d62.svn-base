		// 2. 사업자 번호 상태 조회 API 호출
		$.ajax({

			beforeSend	: function (request) {}, // 공통 js - $.ajaxSetup 자동 셋팅되어, 값 비우기
			url			: strUrl,
			type		: "POST",
			data		: JSON.stringify(data), // json 을 string으로 변환하여 전송
			dataType	: "JSON",
			contentType	: "application/json",
			accept		: "application/json",
			success		: function(result) {

				if( result.status_code === "OK" ) {

					if( result.data[0].valid === "01" ) { // Valid

						alert("<spring:message code='result-message.messages.join.message.brno.auth.success'/>"); // '사업장인증이 정상적으로 승인되었습니다.'

						$("#brnoChkYn").val('Y');
						$("#entNm").val(entNm);			// 상호명
						$("#opbizYmd").val(opbizYmd);	// 개업일자
						$("#rprsvNm").val(rprsvNm);		// 대표자 성명
						$("#brno").val(brno);			// 사업자 등록 번호

						// 3. 인증 확인 후, 해당 사업자 등록번호 조회 - 있는 경우 - 등록된 상호명 셋팅
						fn_brnoChkGetEntNm( brno );

					} else { // Invalid

						alert("<spring:message code='result-message.messages.join.message.brno.auth.error'/>"); // '유효하지 않은 사업자 등록번호입니다. 사업자 정보 확인 후, 다시 시도해 주세요.'
						$("#brnoChkYn").val('N');
						$("#entNm").val('');
						$("#opbizYmd").val('');
						$("#rprsvNm").val('');
						$("#brno").val('');
					}
				}

			},

			error: function(result) {
				alert( "<spring:message code='result-message.messages.common.message.error'/>" );
			}
		});
	}

	/*
	 * 함수명 : fn_brnoChkGetEntNm()
	 * 설   명 : 인증 확인 후, 해당 사업자 등록번호 조회 - 상호명 셋팅
	 */
	function fn_brnoChkGetEntNm( brno ) {

		$.blockUI();

		// 0. 조회 시작
		$.ajax({
			url	 		: "/join/getEntNm"
			, type 		: "POST"
			, dataType 	: "json"
			, data 		: {
							  "brno" : brno
							}
			, success 	: function(result) {

				$.unblockUI();

				if (result.resultCode == "200") {

					// 1. 조회 결과 있으면 값 셋팅
					if ( result.data != null && result.data != "" ) {
						$("#chkEntNm").val(result.data);
						$("#entNm").val(result.data);
					}

				} else {
					alert(result.resultMessage);
				}
			}
		});
	}

	/*
	 * 함수명 : fn_entGrpDpcnChk()
	 * 설   명 : 사업자등록번호 + 그룹ID + 그룹명 중복 체크
	 */
	function fn_entGrpDpcnChk() {

		// 0. 사업자 등록번호 검사
		if($.trim($("#chkBrno").val()) == "") {
			alert("<spring:message code='result-message.messages.common.message.required.data2' arguments='사업자 등록번호'/>") // '{0}를(을) 입력해주세요.'
			$("#chkBrno").focus();
			isValid = false;
			return false;
		}

		// 1. 그룹명 검사
		if($.trim($("#chkEntGrpNm").val()) == "") {
			alert("<spring:message code='result-message.messages.common.message.required.data2' arguments='그룹명'/>") // '{0}를(을) 입력해주세요.'
			$("#chkEntGrpNm").focus();
			isValid = false;
			return false;
		}

		// 2. 그룹ID 검사
		if($.trim($("#chkEntGrpMngNo").val()) == "") {
			alert("<spring:message code='result-message.messages.common.message.required.data2' arguments='그룹ID'/>") // '{0}를(을) 입력해주세요.'
			$("#chkEntGrpMngNo").focus();
			isValid = false;
			return false;
		}

		// 3. 중복 확인 시작
		$.blockUI();

		let brno 		= $("#chkBrno").val();
		let entGrpNm	= $("#chkEntGrpNm").val();
		let entGrpMngNo	= $("#chkEntGrpMngNo").val();

		$.ajax({
			url	 		: "/join/entGrpDpcnChk"
			, type 		: "POST"
			, dataType 	: "json"
			, data 		: {
							  "brno" 		: brno
							, "entGrpNm"	: entGrpNm
							, "entGrpMngNo" : entGrpMngNo
							}
			, success 	: function(result) {

				$.unblockUI();

				if (result.resultCode == "200") {
					// 비교한 값
					if ( result.data ) {
						alert(result.resultMessage.replaceAll("\\n" , "\\\n").replaceAll("\\", ""));
						$("#entGrpDpcnChkYn").val('Y');
						$("#entGrpNm").val(entGrpNm);
						$("#entGrpMngNo").val(entGrpMngNo);
					} else {
						alert(result.resultMessage.replaceAll("\\n" , "\\\n").replaceAll("\\", ""));
						$("#entGrpDpcnChkYn").val('N');
						$("#entGrpNm").val('');
						$("#entGrpMngNo").val('');
						$("#chkEntGrpMngNo").focus();
					}
				} else {
					alert(result.resultMessage);
				}
			}
		});

	}

	/*
	 * 함수명 : fn_fileOpen()
	 * 설   명 : 해당 첨부파일 창 선택
	 */
	function fn_fileOpen(fileId) {
		$("#" + fileId).click();
	}


