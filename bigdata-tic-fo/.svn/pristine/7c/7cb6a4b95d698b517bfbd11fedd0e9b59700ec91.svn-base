				)
			) a
	) g
LEFT OUTER JOIN
	(
		SELECT
			fn_date_fmt(cert.cert_ymd, 'YY.MM') as cert_ym
			, 1 as cnt
		FROM
			pt_rf_cert_bas cert	/*인증서기본*/
		INNER JOIN
			pt_rf_cert_rcpt_rlt rlt	/*인증서접수관계*/
			on cert.inst_id = rlt.inst_id	/*기관아이디*/
			and cert.cert_no = rlt.cert_no	/*인증번호*/
			<include refid='myDataStatCertWhere'>
