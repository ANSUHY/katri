@charset "utf-8";

body,td,th {
	font-family: "나눔고딕";
	font-size: 10px;
	background-color: #fafafa;
}
body, div {
	margin:0;
    padding:0;
    border:0;
    outline:0;
    font-size:100%;	
    }
html, body, form, #wrap {
    height:100%;
    }
body > #wrap {
    height: 100%;
    min-height:100%;
    }
#wrap{
	margin:0 auto;
	width: 100%;
	background-repeat: no-repeat;
	background-position: center;
    }
